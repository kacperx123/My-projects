#include <iostream>
#include <cmath>
#include <math.h>
using namespace std;

int main()
{
    int n = 10000, ile;
    bool wynik[n + 1];
    for(int i = 0; i<=n; i++)
    {
        wynik[i]=true;
    }
    wynik[1] = false;
    for (int i = 2; i<= sqrt(n); i++)
    {
        if(wynik[i] == true)
        {
        for(int j=i+i; j<=n; j=j+i)
            {
                wynik[j]=false;
            }
        }
    }
    cin >> ile;
    for(int i = 0; i<ile; i++)
    {
        int liczba;
        cin >> liczba;
        if (wynik[liczba] == true)
        {
            cout << "TAK"<< endl;
        }
        else
        {
            cout << "NIE"<< endl;
        }
    }
    return 0;
}
