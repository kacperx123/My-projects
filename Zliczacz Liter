#include <iostream>
#include <string>

using namespace std;

int main()
{
    std::ios_base::sync_with_stdio(false);
    int dlugosc;
    int literki[200] = {0};
    int ile ;
    int z = 0;
    string pomoc;
    string piosenka;
        cin >> ile;
       do {
        getline(cin,pomoc);
        piosenka = piosenka + pomoc;
        z++;
         }
       while(ile >= z);
        dlugosc = piosenka.length();
        for(int i = 0 ; i < dlugosc ; i++)
        {
            literki[piosenka[i]]++;
        }
        for(int i =97 ; i < 123 ; i++)
        {
            if(literki[i] > 0)
            {
            cout << (char)i <<" "<< literki[i]<<endl;
            }
        }
        for(int i =65 ; i < 91 ; i++)
        {
            if(literki[i] > 0)
            {
               cout << (char)i <<" "<< literki[i]<<endl;
            }

        }
    return 0;
}
